# Register 33
record(calc, "$(P)$(R)OTP00to05StateCalc") {
#  field(ASG, "Reserved")
  field(DESC, "OTP 00 to 05 State Enbl OR flag")
  field(INPA, "$(P)$(R)OTP00State-Sts.VAL CPP")
  field(INPB, "$(P)$(R)OTP01State-Sts.VAL CPP")
  field(INPC, "$(P)$(R)OTP02State-Sts.VAL CPP")
  field(INPD, "$(P)$(R)OTP03State-Sts.VAL CPP")
  field(INPE, "$(P)$(R)OTP04State-Sts.VAL CPP")
  field(INPF, "$(P)$(R)OTP05State-Sts.VAL CPP")
  field(INPG, "$(P)$(R)OTP00EvtCntRst-Cmd.VAL CPP")
  field(INPH, "$(P)$(R)OTP01EvtCntRst-Cmd.VAL CPP")
  field(INPI, "$(P)$(R)OTP02EvtCntRst-Cmd.VAL CPP")
  field(INPJ, "$(P)$(R)OTP03EvtCntRst-Cmd.VAL CPP")
  field(INPK, "$(P)$(R)OTP04EvtCntRst-Cmd.VAL CPP")
  field(INPL, "$(P)$(R)OTP05EvtCntRst-Cmd.VAL CPP")
  field(CALC, "A || B || C || D || E || F || G || H || I || J || K || L")
  field(FLNK, "$(P)$(R)OTPCntReadSELNCalc")
}

