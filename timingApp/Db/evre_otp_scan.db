record(calc, "$(P)$(R)OTPCntReadSELNCalc") {
#  field(ASG, "Reserved")
  field(DESC, "OTP Count Read ")
  field(INPA, "$(P)$(R)OTP00to05StateCalc")
  field(INPB, "$(P)$(R)OTP06to11StateCalc")
  field(INPC, "$(P)$(R)OTP12to17StateCalc")
  field(INPD, "$(P)$(R)OTP18to23StateCalc")
  field(CALC, "A+B*2+C*4+D*8")
#  field(FLNK, "$(P)$(R)OTPCntReadFanout")
}

record(fanout, "$(P)$(R)OTPCntReadFanout") {
#  field(ASG, "Reserved")
  field(DESC, "Read count register fanout")
  field(SCAN, ".1 second")
  field(SELL, "$(P)$(R)OTPCntReadSELNCalc")
  field(SELM, "Mask")
  field(LNK1, "$(P)$(R)cmd_evre_otp00to05_count_get")
  field(LNK2, "$(P)$(R)cmd_evre_otp06to11_count_get")
  field(LNK3, "$(P)$(R)cmd_evre_otp12to17_count_get")
  field(LNK4, "$(P)$(R)cmd_evre_otp18to23_count_get")
}

record(longout, "$(P)$(R)cmd_evre_otp00to05_count_get") {
#  field(ASG, "Reserved")
  field(DESC, "Command code to read Ctrl Status reg")
  field(VAL, "0xA1") # (0x80 | 33)
  field(DTYP, "stream")
  field(OUT, "@timing.proto evre_otp00to05_count_get($(P),$(R)) $(PORT)")
}

