    evg_mux_get;
}

################################################################################
# SEQRAM setting register [49]
evg_seqramset_get {
    out "%(\$1:cmd_evg_seqramset_get)r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "\xF1%*02r%(\$1:seqaddrRBV.RVAL)02r%*03r%(\$1:seqcodeRBV.RVAL)r%(\$1:seqtimeRBV)04r";
}

evg_seqramset_set{
    out "%(\$1:cmd_evg_seqramset_set).1r\x00\x00%(\$1:seqaddr).2r\x00\x00\x00%(\$1:seqcode).1r%(\$1:seqtime).4r";
    
    #evg_seqramsw_get
    out "%(\$1:cmd_evg_seqramsw_get)r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "\xF2%*04r%*04r%*02r%(\$1:seqcountRBV.RVAL)02r";
}

