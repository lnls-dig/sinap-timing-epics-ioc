    evre_ctrl_set;
}

evg_download{
	evg_conf_set; # initialize the module
    out "%(\$1:cmd_evg_mux_set0).1r\x00\x00\x00%(\$1:muxen0).1r\x00\x00\x00\x00%(\$1:muxdiv0).4r";
    out "%(\$1:cmd_evg_mux_set1).1r\x00\x00\x00%(\$1:muxen1).1r\x00\x00\x00\x00%(\$1:muxdiv1).4r";
    out "%(\$1:cmd_evg_mux_set2).1r\x00\x00\x00%(\$1:muxen2).1r\x00\x00\x00\x00%(\$1:muxdiv2).4r";
    out "%(\$1:cmd_evg_mux_set3).1r\x00\x00\x00%(\$1:muxen3).1r\x00\x00\x00\x00%(\$1:muxdiv3).4r";
    out "%(\$1:cmd_evg_mux_set4).1r\x00\x00\x00%(\$1:muxen4).1r\x00\x00\x00\x00%(\$1:muxdiv4).4r";
    out "%(\$1:cmd_evg_mux_set5).1r\x00\x00\x00%(\$1:muxen5).1r\x00\x00\x00\x00%(\$1:muxdiv5).4r";
    out "%(\$1:cmd_evg_mux_set6).1r\x00\x00\x00%(\$1:muxen6).1r\x00\x00\x00\x00%(\$1:muxdiv6).4r";
    out "%(\$1:cmd_evg_mux_set7).1r\x00\x00\x00%(\$1:muxen7).1r\x00\x00\x00\x00%(\$1:muxdiv7).4r";
    evg_acline_set;
    evg_ctrl_set;
}

