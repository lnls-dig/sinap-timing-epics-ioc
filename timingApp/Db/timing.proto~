Terminator = "";
MaxInput = 13;
ExtraInput = Ignore;
################################################################################
# Control and Status Register EVE & EVR [0]
evre_ctrl_get {
    out "%(\$1:cmd_evre_ctrl_get)r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "\xc0%*04r%*04r%*02r%(\$1:linkinhs.RVAL)r%(\$1:enableRAW.RVAL)r";
}

evre_ctrl_set {
    out "%(\$1:cmd_evre_ctrl_set)r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00%(\$1:enable)b";
    evre_ctrl_get;
}

################################################################################
# Control and Status Register EVG [0]
evg_ctrl_get {
    out "%(\$1:cmd_evg_ctrl_get)r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "\xc0%*04r%*04r%*02r%(\$1:seqstat_rfstat.RVAL)r%(\$1:seqen_enable.RVAL)r";
}

evg_ctrl_seq_en_set{
    out "%(\$1:cmd_evg_ctrl_set).1r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00%(\$2).1r";
    evg_ctrl_get;
}

evg_ctrl_set{
    out "%(\$1:cmd_evg_ctrl_set).1r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00%(\$1:SEQEN_ENABLE).1r";
    evg_ctrl_get;
}

################################################################################
# OTP Registers [1 - 16]
evre_otp_get {
    out "%(\$1:cmd_evre_otp_get\$2)r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "%*r%(\$1:enpol\$2RBV.RVAL)r%*02r%(\$1:event\$2RBV)r%(\$1:delay\$2RBV)04r%(\$1:width\$2RBV)04r";
}

evre_otp_ev_set {
    out "%(\$1:cmd_evre_otp_set\$2)r%(\$1:enpol\$2RBV)r\x00\x00%(\$1:event\$2)r%(\$1:delay\$2RBV).4r%(\$1:width\$2RBV).4r";
    evre_otp_get;
}

evre_otp_width_set {
    out "%(\$1:cmd_evre_otp_set\$2)r%(\$1:enpol\$2RBV)r\x00\x00%(\$1:event\$2RBV)r%(\$1:delay\$2RBV).4r%(\$1:width\$2).4r";
    evre_otp_get;
}

evre_otp_dly_set {
    out "%(\$1:cmd_evre_otp_set\$2)r%(\$1:enpol\$2RBV)r\x00\x00%(\$1:event\$2RBV)r%(\$1:delay\$2).4r%(\$1:width\$2RBV).4r";
    evre_otp_get;
}

evre_otp_en_pol_set {
    out "%(\$1:cmd_evre_otp_set\$2)r%(\$1:\$3\$2)r\x00\x00%(\$1:event\$2RBV)r%(\$1:delay\$2RBV).4r%(\$1:width\$2RBV).4r";
    evre_otp_get;
}

evre_otp_set {
    out "%(\$1:cmd_evre_otp_set\$2)r%(\$1:enpol\$2)r\x00\x00%(\$1:event\$2)r%(\$1:delay\$2).4r%(\$1:width\$2).4r";
    evre_otp_get;
}

################################################################################
# OUT Registers [17 - 24]
evre_out_get {
    out "%(\$1:cmd_evre_out_get\$2)r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "%*r%(\$1:itl\$2RAW.RVAL)r%*02r%(\$1:sel\$2RBV.RVAL)r%*02r%(\$1:finedly\$2RBV.RVAL)02r%*03r%(\$1:rfdly\$2RBV.RVAL)r";
}

evre_out_rfdly_set{
    out "%(\$1:cmd_evre_out_set\$2).1r%(\$1:itl\$2RAW.RVAL).1r\x00\x00%(\$1:sel\$2RBV.RVAL).1r\x00\x00%(\$1:finedly\$2RBV.RVAL).2r\x00\x00\x00%(\$1:rfdly\$2).1r";
    evre_out_get;
}

evre_out_finedly_set{
    out "%(\$1:cmd_evre_out_set\$2).1r%(\$1:itl\$2RAW.RVAL).1r\x00\x00%(\$1:sel\$2RBV.RVAL).1r\x00\x00%(\$1:finedly\$2).2r\x00\x00\x00%(\$1:rfdly\$2RBV.RVAL).1r";
    evre_out_get;
}

evre_out_sel_set{
    out "%(\$1:cmd_evre_out_set\$2).1r%(\$1:itl\$2RAW.RVAL).1r\x00\x00%(\$1:sel\$2).1r\x00\x00%(\$1:finedly\$2RBV.RVAL).2r\x00\x00\x00%(\$1:rfdly\$2RBV.RVAL).1r";
    evre_out_get;
}

evre_out_itl_set{
    out "%(\$1:cmd_evre_out_set\$2).1r%(\$1:ITL\$2).1r\x00\x00%(\$1:sel\$2RBV.RVAL).1r\x00\x00%(\$1:finedly\$2RBV.RVAL).2r\x00\x00\x00%(\$1:rfdly\$2RBV.RVAL).1r";
    evre_out_get;
}

evre_out_set{
    out "%(\$1:cmd_evre_out_set\$2).1r%(\$1:ITL\$2).1r\x00\x00%(\$1:sel\$2).1r\x00\x00%(\$1:finedly\$2).2r\x00\x00\x00%(\$1:rfdly\$2).1r";
    evre_out_get;
}

################################################################################
# Clock mode select [25]
eve_rf_get {
    out "\x99\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "\xd9%*04r%*04r%*03r%(\$1:cmselRBV)r";
}

eve_rf_set {
    out "\x59\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00%(\$1:cmsel)r";
    eve_rf_get;
}

################################################################################
# AC Line register [40]
evg_acline_get {
    out "%(\$1:cmd_evg_acline_get)r\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "\xE8%*03r%(\$1:acenRBV)r%*04r%(\$1:acdivRAW)04r";
}

