    evg_acline_get;
}

evg_acline_acdiv_set{
    out "%(\$1:cmd_evg_acline_set).1r\x00\x00\x00%(\$1:acenRBV).1r\x00\x00\x00\x00%(\$1:acdiv.RVAL).4r";
    evg_acline_get;
}

evg_acline_set{
    out "%(\$1:cmd_evg_acline_set).1r\x00\x00\x00%(\$1:acen).1r\x00\x00\x00\x00%(\$1:acdiv.RVAL).4r";
    evg_acline_get;
}

