################################################################################
# Configutarion register [63]
eve_conf_get {
    out "\xBF\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "\xFF%(\$1:alive)04r%*03r%(\$1:clkmodeRBV)r%*03r%(\$1:funselRBV)r";
}

evr_conf_get {
    out "\xBF\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "\xFF%(\$1:alive)04r%*03r%(\$1:clkmodeRBV)r%*03r%(\$1:funselRBV)r";
}

evg_conf_get {
    out "\xBF\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00";
    in "\xFF%(\$1:alive)04r%*r%(\$1:rfdivRAW)r%*02r%*03r%(\$1:funselRBV)r";
}

evg_conf_funsel_set{
    out "%(\$1:cmd_evg_conf_set).1r\x00\x00\x00\x00\x00%(\$1:rfdivRAW).1r\x00\x00\x00\x00\x00%(\$1:funsel).1r";
    evg_conf_get;
}

evg_conf_rfdiv_set{
    out "%(\$1:cmd_evg_conf_set).1r\x00\x00\x00\x00\x00%(\$1:rfdiv.RVAL).1r\x00\x00\x00\x00\x00%(\$1:funselRBV).1r";
    evg_conf_get;
}

evg_conf_set{
    out "%(\$1:cmd_evg_conf_set).1r\x00\x00\x00\x00\x00%(\$1:rfdiv.RVAL).1r\x00\x00\x00\x00\x00%(\$1:funsel).1r";
    evg_conf_get;
}

eve_conf_set{
    out "%(\$1:cmd_eve_conf_set).1r\x00\x00\x00\x00\x00\x00\x00%(\$1:clkmode).1r\x00\x00\x00\x20";
    eve_conf_get;
}

evr_conf_funsel_set{
    out "%(\$1:cmd_evr_conf_set).1r\x00\x00\x00\x00\x00\x00\x00%(\$1:clkmodeRBV).1r\x00\x00\x00%(\$1:funsel).1r";
    evr_conf_get;
}

evr_conf_clk_set{
    out "%(\$1:cmd_evr_conf_set).1r\x00\x00\x00\x00\x00\x00\x00%(\$1:clkmode).1r\x00\x00\x00%(\$1:funselRBV).1r";
    evr_conf_get;
}

evr_conf_set{
    out "%(\$1:cmd_evr_conf_set).1r\x00\x00\x00\x00\x00\x00\x00%(\$1:clkmode).1r\x00\x00\x00%(\$1:funsel).1r";
    evr_conf_get;
}

########################################################################
# Download commands (From IOC to Hardware)
eve_download{
	eve_conf_set; # initialize the module
	eve_rf_set;
    out "%(\$1:cmd_evre_otp_set0)r%(\$1:enpol0)r\x00\x00%(\$1:event0)r%(\$1:delay0).4r%(\$1:width0).4r";
    out "%(\$1:cmd_evre_otp_set1)r%(\$1:enpol1)r\x00\x00%(\$1:event1)r%(\$1:delay1).4r%(\$1:width1).4r";
    out "%(\$1:cmd_evre_otp_set2)r%(\$1:enpol2)r\x00\x00%(\$1:event2)r%(\$1:delay2).4r%(\$1:width2).4r";
    out "%(\$1:cmd_evre_otp_set3)r%(\$1:enpol3)r\x00\x00%(\$1:event3)r%(\$1:delay3).4r%(\$1:width3).4r";
    out "%(\$1:cmd_evre_otp_set4)r%(\$1:enpol4)r\x00\x00%(\$1:event4)r%(\$1:delay4).4r%(\$1:width4).4r";
    out "%(\$1:cmd_evre_otp_set5)r%(\$1:enpol5)r\x00\x00%(\$1:event5)r%(\$1:delay5).4r%(\$1:width5).4r";
    out "%(\$1:cmd_evre_otp_set6)r%(\$1:enpol6)r\x00\x00%(\$1:event6)r%(\$1:delay6).4r%(\$1:width6).4r";
    out "%(\$1:cmd_evre_otp_set7)r%(\$1:enpol7)r\x00\x00%(\$1:event7)r%(\$1:delay7).4r%(\$1:width7).4r";
    out "%(\$1:cmd_evre_otp_set8)r%(\$1:enpol8)r\x00\x00%(\$1:event8)r%(\$1:delay8).4r%(\$1:width8).4r";
    out "%(\$1:cmd_evre_otp_set9)r%(\$1:enpol9)r\x00\x00%(\$1:event9)r%(\$1:delay9).4r%(\$1:width9).4r";
    out "%(\$1:cmd_evre_otp_setA)r%(\$1:enpolA)r\x00\x00%(\$1:eventA)r%(\$1:delayA).4r%(\$1:widthA).4r";
    out "%(\$1:cmd_evre_otp_setB)r%(\$1:enpolB)r\x00\x00%(\$1:eventB)r%(\$1:delayB).4r%(\$1:widthB).4r";
    out "%(\$1:cmd_evre_otp_setC)r%(\$1:enpolC)r\x00\x00%(\$1:eventC)r%(\$1:delayC).4r%(\$1:widthC).4r";
    out "%(\$1:cmd_evre_otp_setD)r%(\$1:enpolD)r\x00\x00%(\$1:eventD)r%(\$1:delayD).4r%(\$1:widthD).4r";
    out "%(\$1:cmd_evre_otp_setE)r%(\$1:enpolE)r\x00\x00%(\$1:eventE)r%(\$1:delayE).4r%(\$1:widthE).4r";
    out "%(\$1:cmd_evre_otp_setF)r%(\$1:enpolF)r\x00\x00%(\$1:eventF)r%(\$1:delayF).4r%(\$1:widthF).4r";
    out "%(\$1:cmd_evre_out_set0).1r%(\$1:ITL0).1r\x00\x00%(\$1:sel0).1r\x00\x00%(\$1:finedly0).2r\x00\x00\x00%(\$1:rfdly0).1r";
    out "%(\$1:cmd_evre_out_set1).1r%(\$1:ITL1).1r\x00\x00%(\$1:sel1).1r\x00\x00%(\$1:finedly1).2r\x00\x00\x00%(\$1:rfdly1).1r";
    out "%(\$1:cmd_evre_out_set2).1r%(\$1:ITL2).1r\x00\x00%(\$1:sel2).1r\x00\x00%(\$1:finedly2).2r\x00\x00\x00%(\$1:rfdly2).1r";
    out "%(\$1:cmd_evre_out_set3).1r%(\$1:ITL3).1r\x00\x00%(\$1:sel3).1r\x00\x00%(\$1:finedly3).2r\x00\x00\x00%(\$1:rfdly3).1r";
    out "%(\$1:cmd_evre_out_set4).1r%(\$1:ITL4).1r\x00\x00%(\$1:sel4).1r\x00\x00%(\$1:finedly4).2r\x00\x00\x00%(\$1:rfdly4).1r";
    out "%(\$1:cmd_evre_out_set5).1r%(\$1:ITL5).1r\x00\x00%(\$1:sel5).1r\x00\x00%(\$1:finedly5).2r\x00\x00\x00%(\$1:rfdly5).1r";
    out "%(\$1:cmd_evre_out_set6).1r%(\$1:ITL6).1r\x00\x00%(\$1:sel6).1r\x00\x00%(\$1:finedly6).2r\x00\x00\x00%(\$1:rfdly6).1r";
    out "%(\$1:cmd_evre_out_set7).1r%(\$1:ITL7).1r\x00\x00%(\$1:sel7).1r\x00\x00%(\$1:finedly7).2r\x00\x00\x00%(\$1:rfdly7).1r";
    evre_ctrl_set;
}

evr_download{
	evr_conf_set; # initialize the module
    out "%(\$1:cmd_evre_otp_set0)r%(\$1:enpol0)r\x00\x00%(\$1:event0)r%(\$1:delay0).4r%(\$1:width0).4r";
    out "%(\$1:cmd_evre_otp_set1)r%(\$1:enpol1)r\x00\x00%(\$1:event1)r%(\$1:delay1).4r%(\$1:width1).4r";
    out "%(\$1:cmd_evre_otp_set2)r%(\$1:enpol2)r\x00\x00%(\$1:event2)r%(\$1:delay2).4r%(\$1:width2).4r";
    out "%(\$1:cmd_evre_otp_set3)r%(\$1:enpol3)r\x00\x00%(\$1:event3)r%(\$1:delay3).4r%(\$1:width3).4r";
    out "%(\$1:cmd_evre_otp_set4)r%(\$1:enpol4)r\x00\x00%(\$1:event4)r%(\$1:delay4).4r%(\$1:width4).4r";
    out "%(\$1:cmd_evre_otp_set5)r%(\$1:enpol5)r\x00\x00%(\$1:event5)r%(\$1:delay5).4r%(\$1:width5).4r";
    out "%(\$1:cmd_evre_otp_set6)r%(\$1:enpol6)r\x00\x00%(\$1:event6)r%(\$1:delay6).4r%(\$1:width6).4r";
    out "%(\$1:cmd_evre_otp_set7)r%(\$1:enpol7)r\x00\x00%(\$1:event7)r%(\$1:delay7).4r%(\$1:width7).4r";
    out "%(\$1:cmd_evre_otp_set8)r%(\$1:enpol8)r\x00\x00%(\$1:event8)r%(\$1:delay8).4r%(\$1:width8).4r";
